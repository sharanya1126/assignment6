      "source": [
        "b_num=list(input(\"input a binary number:\"))\n",
        "value=0\n",
        "\n",
        "for i in range(len(b_num)):\n",
        "    digit=b_num.pop()\n",
        "    if digit=='1':\n",
        "      value=value+pow(2,i)\n",
        "      print(\"the decimal value of the number is\",value)"
      ],

        "N=int(input(\"enter the value of N:\"))\n",
        "f,s=0,1\n",
        "i=0\n",
        "while(i<N):\n",
        "   next=f+s\n",
        "   f=s\n",
        "   s=next\n",
        "   i=i+1\n",
        "   print(next)\n",
